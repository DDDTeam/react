{
  "name": "@robocotik/react",
  "version": "1.0.20",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "build": "npm run type-check && rollup -c",
    "build:watch": "npm run type-check && rollup -c -w",
    "dev": "npm run type-check && rollup -c -w --serve"
  },
  "keywords": [
    "react",
    "ddd",
    "library",
    "frontend",
    "typescript"
  ],
  "author": "Robocotik",
  "main": "dist/react.js",
  "types": "dist/react.d.ts",
  "typings": "dist/react.d.ts",
  "module": "dist/react.js",
  "exports": {
    ".": {
      "import": "./dist/react.js",
      "require": "./dist/react.js",
      "types": "./dist/react.d.ts"
    },
    "./jsx-runtime": {
      "import": "./dist/jsx/jsx-runtime.js",
      "require": "./dist/jsx/jsx-runtime.js",
      "types": "./dist/jsx/jsx-runtime.d.ts"
    },
    "./jsx-dev-runtime": {
      "import": "./dist/jsx/jsx-dev-runtime.js",
      "require": "./dist/jsx/jsx-dev-runtime.js",
      "types": "./dist/jsx/jsx-dev-runtime.d.ts"
    }
  },
  "license": "ISC",
  "description": "The minimalistic React-like library made by DDD team",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/node": "^24.8.1",
    "rollup": "^4.52.4",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.2.3",
    "rollup-plugin-filesize": "^10.0.0",
    "typescript": "^5.9.3",
    "tslib": "^2.8.1"
  },
  "dependencies": {
    "@guanghechen/fast-deep-equal": "^2.3.7"
  },
  "files": [
    "dist/**/*",
    "README.md"
  ]
}